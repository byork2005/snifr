<div id="dogContainer">
				{{#each Dog}}
                	<div class="row" id="yourDogs">
                		<div class="col col-sm-2">
	                		<img src="{{this.photo}}" id="dogImg">
	                	</div>
	                	<div class="col col-sm-6">
							
	                		<h2 id="dogName">{{this.name}}</h2>
	                		<hr>
	                		<h3 id="dogHuman">Human's Name: </h3>
	                	</div>
	                	<div class="col col-sm-4">
	                		<button type="button" class="btn btn-success" id="edit">Edit</button>
	                	</div>

                	</div>
					
					{{/each}}
					
                    
                </div>